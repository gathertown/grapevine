{
  "name": "@corporate-context/js-services",
  "version": "1.0.0",
  "description": "Corporate Context JavaScript Services Workspace",
  "private": true,
  "type": "module",
  "workspaces": [
    "admin-backend",
    "admin-frontend",
    "backend-common",
    "exponent-core",
    "exponent-evals",
    "frontend-common",
    "gather-design-foundations",
    "gather-design-system",
    "localstack-ui-backend",
    "localstack-ui-frontend",
    "shared-common",
    "slack-bot"
  ],
  "scripts": {
    "dev": "nx run-many --target=dev --parallel",
    "dev:admin": "nx run-many --target=dev --projects=admin-backend,admin-frontend --parallel",
    "dev:localstack": "nx run-many --target=dev --projects=localstack-ui-backend,localstack-ui-frontend --parallel",
    "build": "nx run-many --target=build --all --parallel",
    "build:affected": "nx affected:build",
    "serve": "nx run-many --target=serve --parallel",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint-fix --all",
    "lint:affected": "nx affected:lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "type-check": "nx run-many --target=type-check --all",
    "type-check:affected": "nx affected --target=type-check",
    "test": "nx run-many --target=test --all --parallel",
    "test:affected": "nx affected:test",
    "knip": "knip",
    "knip:admin-frontend": "knip --include files --workspace admin-frontend",
    "knip:prune-admin-frontend": "knip --fix --fix-type files --workspace admin-frontend --allow-remove-files && echo 'üóëÔ∏è  Admin-frontend components pruned'",
    "knip:prune-admin-safe": "yarn knip:prune-admin-frontend && yarn build && echo '‚úÖ Admin-frontend cleanup completed safely'",
    "graph": "nx graph",
    "reset": "nx reset",
    "clean": "nx reset && rm -rf */dist .nx/cache node_modules/.cache"
  },
  "keywords": [
    "workspace",
    "typescript",
    "corporate-context"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.856.0",
    "@aws-sdk/client-sqs": "^3.856.0",
    "@aws-sdk/lib-storage": "^3.856.0",
    "@aws-sdk/s3-request-presigner": "^3.856.0",
    "@notionhq/client": "^5.1.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "oidc-client-ts": "^3.3.0",
    "pg": "^8.16.3",
    "ramda": "^0.31.3",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.5",
    "stripe": "^18.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@nx/js": "^21.4.1",
    "@nx/workspace": "^21.4.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "@types/ramda": "^0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-verb-nouns": "^1.0.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "knip": "^5.63.1",
    "nodemon": "^3.0.0",
    "nx": "^21.4.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@4.11.0"
}

{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2022",
    "transform": {
      "decoratorMetadata": true,
      "react": {
        "runtime": "automatic"
      }
    },
    "keepClassNames": true,
    "externalHelpers": false
  },
  "module": {
    "type": "es6",
    "strict": true,
    "strictMode": true
  },
  "sourceMaps": true,
  "minify": false
}
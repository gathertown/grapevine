{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "name": "localstack-ui-frontend",
  "sourceRoot": "localstack-ui-frontend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "tsc && vite build",
        "cwd": "localstack-ui-frontend"
      }
    },
    "serve:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "localstack-ui-frontend"
      }
    },
    "type-check": {},
    "lint": {},
    "lint-fix": {},
    "format": {},
    "format:check": {},
    "test": {}
  }
}

{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "name": "shared-common",
  "sourceRoot": "shared-common/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "shared-common/dist",
        "main": "shared-common/src/index.ts",
        "tsConfig": "shared-common/tsconfig.json",
        "assets": [],
        "stripLeadingPaths": true,
        "swcrc": "shared-common/.swcrc",
        "skipTypeCheck": false
      },
      "configurations": {
        "production": {
          "optimization": true,
          "sourceMap": true
        }
      }
    },
    "type-check": {},
    "lint": {},
    "lint-fix": {},
    "format": {},
    "format:check": {},
    "test": {}
  },
  "tags": ["shared", "types"]
}
